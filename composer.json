{
  "name": "aclips/code-scanner",
  "description": "A PHP tool for scanning codebase and extracting information about classes, methods, and functions.",
  "type": "project",
  "require": {
    "php": "^8.0",
    "mongodb/mongodb": "^1.9",
    "monolog/monolog": "^3.0",
    "nikic/php-parser": "^5.3",
    "symfony/console": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Aclips\\CodeScanner\\": "src/Aclips/CodeScanner/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aclips\\CodeScanner\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
